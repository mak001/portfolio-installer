{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "^3.5",
		"silverstripe/framework": "^3.5",
		"silverstripe/reports": "^3.5",
		"silverstripe/siteconfig": "^3.5",
		
		"axllent/silverstripe-trailing-slash": "dev-master",
		"silverstripe/googlesitemaps": "1.6.0",
		"mediaweb/silverstripe-tinymce4": "^1.1",
	
		"mak001/portfolio": "1.0.x-dev",
		"mak001/portfolio-theme": "1.0.x-dev"
	},
	"repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mak001/portfolio"
        },
		{
            "type": "vcs",
            "url": "https://github.com/mak001/portfolio-theme"
        }
    ],
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.5.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
